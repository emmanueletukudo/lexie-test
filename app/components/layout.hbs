<div class="app">
<LinkTo @route="cart" class="cart-link">
  <i class="bi bi-cart"></i>
    {{#if this.cartService.itemList.length}}
     <span class="cart-count d-inline justify-content-center align-items-center">
      {{this.itemCount}}
     </span>
    {{/if}}
</LinkTo>
<main class="container m-5" >
  <div class="home-wrapper">
    {{yield}}
  </div>
</main>
</div>


